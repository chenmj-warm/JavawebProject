package com.lanhai.pojo;

import java.util.Date;

public class DiaoCha {

	//调查表
	private Integer plId;
	private Integer userId;
	
	private Integer wjId;
	
	
	//评论信息
	private String plInner;
	//评论时间
	private Date plTime;
	public DiaoCha() {
		super();
		// TODO Auto-generated constructor stub
	}
	public DiaoCha(Integer plId, Integer userId, Integer wjId, String plInner, Date plTime) {
		super();
		this.plId = plId;
		this.userId = userId;
		this.wjId = wjId;
		this.plInner = plInner;
		this.plTime = plTime;
	}
	public DiaoCha(Integer userId, Integer wjId, String plInner, Date plTime) {
		super();
		this.userId = userId;
		this.wjId = wjId;
		this.plInner = plInner;
		this.plTime = plTime;
	}
	@Override
	public String toString() {
		return "DiaoCha [plId=" + plId + ", userId=" + userId + ", wjId=" + wjId + ", plInner=" + plInner + ", plTime="
				+ plTime + "]";
	}
	public Integer getPlId() {
		return plId;
	}
	public void setPlId(Integer plId) {
		this.plId = plId;
	}
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	public Integer getWjId() {
		return wjId;
	}
	public void setWjId(Integer wjId) {
		this.wjId = wjId;
	}
	public String getPlInner() {
		return plInner;
	}
	public void setPlInner(String plInner) {
		this.plInner = plInner;
	}
	public Date getPlTime() {
		return plTime;
	}
	public void setPlTime(Date plTime) {
		this.plTime = plTime;
	}
	
	
}
